{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "electron-starter.js",
  "homepage": "/dashboard",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "apisauce": "^0.15.1",
    "base-64": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.2.5",
    "clsx": "^1.0.4",
    "crypto-js": "^3.1.9-1",
    "localforage": "^1.7.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "namor": "^1.0.1",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "ramdasauce": "^2.1.0",
    "react": "^16.5.1",
    "react-datepicker": "^1.8.0",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-image-gallery": "^0.8.14",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-responsive-carousel": "^3.1.47",
    "react-router-dom": "^5.0.1",
    "react-router-transition": "^1.2.1",
    "react-scripts": "1.1.4",
    "react-table": "^6.10.3",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "reduxsauce": "^1.0.0",
    "reselect": "^3.0.1",
    "seamless-immutable": "^7.1.3",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.1",
    "swagger-ui": "^3.22.0",
    "universal-cookie": "^3.1.0"
  },
  "scripts": {
    "start": "cross-env react-scripts start",
    "start:dev": "cross-env REACT_APP_ENV=development npm start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "jest": "jest",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "format": "prettier-standard 'src/**/*.js'",
    "lint": "prettier-standard 'src/**/*.js'",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "start:webdev": "NODE_ENV=development PORT=3000 react-scripts start",
    "start:webprod": "NODE_ENV=production PORT=3000 react-scripts start",
    "start:electrondev": "NODE_ENV=development electron .",
    "start:electronprod": "NODE_ENV=production electron .",
    "start:electronproddebug": "DEBUG_PROD=true NODE_ENV=production electron .",
    "build:production": "NODE_ENV=production webpack --optimize-minimize"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-links": "^3.4.6",
    "@storybook/addons": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "app-root-path": "^2.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "cross-env": "^5.2.1",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "escope": "^3.6.0",
    "format-json": "^1.0.3",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "json2csv": "^4.1.2",
    "jsonwebtoken": "^8.4.0",
    "lint-staged": "^4.0.1",
    "nedb": "^1.8.0",
    "prettier-standard": "^8.0.1",
    "react-test-renderer": "^16.5.1",
    "reactotron-react-js": "^1.13.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "1.4.0",
    "standard": "^11.0.1",
    "uuid": "^3.2.1",
    "xlsx": "^0.17.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "standard",
      "prettier-standard --single-quote --no-semi",
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "jasmine",
      "beforeAll",
      "afterAll",
      "test",
      "it",
      "expect",
      "describe",
      "__DEV__"
    ]
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost:8090/",
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.js",
      "<rootDir>/src/Containers/*/Tests/*.js",
      "<rootDir>/src/Components/*/Tests/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/Tests/Setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/Tests/Setup.js"
    ]
  }
}
